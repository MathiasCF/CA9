doi = {10.1016/j.apenergy.2020.116286},
file = {:C\:/Users/Mathias/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Saletti et al. - 2021 - Enabling smart control by optimally managing the State of Charge of district heating networks(2).pdf:pdf},
