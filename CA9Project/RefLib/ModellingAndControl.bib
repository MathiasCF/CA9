issn = {14712954},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Frenet-Serret,Hankel matrix,Koopman operator,dynamic mode decomposition,time-delay coordinates},
month = {oct},
number = {2254},
publisher = {
The Royal Society
